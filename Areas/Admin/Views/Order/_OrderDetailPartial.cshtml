@model IEnumerable<WebsiteTMDT.Areas.Admin.Models.EF.OrderDetail>

@if(Model != null && Model.Any())
{
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>#</th>
                <th>Tên sản phẩm</th>
                <th>Giá</th>
                <th>Số lượng</th>
                <th>Thành tiền</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td></td>
                    <td>@item.Product.Title</td>
                    <td>@item.Price</td>
                    <td>@item.Quality</td>
                    <td>@(item.Price * item.Quality)</td>
                </tr>
            }
        </tbody>
    </table>
}